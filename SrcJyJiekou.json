    "csp_xpath_jbb":{//接口标识不能重复
        "name": "剧白白",//接口名称
        "type": "tvbox",//接口类型，tvbox或biubiu
        "filter": "",//过虑字符，使用正则表达式/过虑1|过虑2/g
        "ua": "",
        "dtUrl": "https://www.jubaibai.cc/vod/{vid}.html",//视频地址
        "dtNode": "//body",//xpath最顶层节点
